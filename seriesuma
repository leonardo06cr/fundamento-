#include <iostream>


#include "conio.h"


using namespace std;



float serie(int n,float x);



ã€€


void main(){


	float x,s;


	int n;


	cout<<"ingrese n:";


	cin>>n;


	cout<<"ingrese x:";


	cin>>x;


	s=serie(n,x);


	cout<<"El resultado de la serie es"<<s;


}


float serie(int n,float x){


	float s=5.6;


	int i=1;


	while (i<=n){


		s=s+x/(i+3);


		i++;


	}


	return s;


}


