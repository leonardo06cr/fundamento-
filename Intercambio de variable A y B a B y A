#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;

void main(){
	float A,B,aux;

		cout<<"Ingrese dos numero entero... "<<endl;
		cout<<"Ingrese A:";
		cin>>A;
		cout<<"Ingrese B:";
		cin>>B;


		aux=A;
		A=B;
		B=aux;

		cout<<"el nuevo valor de A es:"<<A<<endl;
		cout<<"El nuevo valor de B:"<<B<<endl;
	

			 getch();
}

*************************************************************************

Proceso intercambiovariable
	Escribir "ingrese a:";
	Leer a;
	Escribir "ingrese b:";
	Leer b;
	c<-a;
	a<-b;
	b<-c;
	Escribir "nuevo a:";
	escribir a;
	Escribir "escribi";
	Escribir b;
	
FinProceso
